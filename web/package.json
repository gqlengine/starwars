{
  "name": "starwars-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "Apache",
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=development parcel index.html",
    "dev": "npm-run-all --parallel dev:build",
    "prod": "yarn prod:build && yarn bundle",
    "clean": "rimraf dist",
    "dev:build": "yarn clean && cross-env NODE_ENV=development parcel watch index.js --out-dir dist/bundles",
    "prod:build": "yarn clean && yarn prod:build-client",
    "prod:build-client": "cross-env NODE_ENV=production parcel build index.html --no-source-maps --public-url / --out-dir dist/bundles --detailed-report",
    "bundle": "go-bindata -fs -pkg main -o ../bindata.go -prefix 'dist/bundles' dist/bundles/*"
  },
  "dependencies": {
    "antd": "^3.26.9",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "postcss-modules": "^1.5.0",
    "prop-types": "latest",
    "react": "^16.12.0",
    "react-amap": "^1.2.8",
    "react-amap-plugin-autocomplete": "^0.0.4",
    "react-amap-plugin-geolocation": "^0.0.2",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-measure": "^2.3.0",
    "react-props": "^0.0.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@svgr/parcel-plugin-svgr": "^5.0.1",
    "babel-plugin-import": "^1.13.0",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-less-js-enabled": "^1.0.2"
  }
}
